{
	"manifest_version": 2,
	"name": "Graphite Events",
	"description": "Push events to a Graphite platform",
	"version": "0.0.3",
	"homepage_url": "https://github.com/LoannNeveu/graphite-events-extension",
	"author": "Loann Neveu <neveu.loann@gmail.com>",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": "icons/icon.png",
		"default_popup": "popup.html",
		"default_title": "Graphite Events"
	},
	"permissions": [
		"activeTab"
		, "storage"
		, "*://*/*"
	]
}
